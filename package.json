{
  "name": "novel",
  "displayName": "novel",
  "description": "novel supporter",
  "version": "0.0.1",
  "publisher": "eirueirufu",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "novel",
        "extensions": [
          ".novel",
          ".nv"
        ],
        "icon": {
          "light": "./media/novel.jpg",
          "dark": "./media/novel.jpg"
        },
        "configuration": "./language-configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "novel",
        "path": "./snippets.json"
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "@types/vscode": "^1.79.1",
    "vscode": "^1.1.37"
  }
}